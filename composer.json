{
    "name": "cakephp/cakephp-api-docs",
    "description": "API docs for CakePHP",
    "type": "application",
    "require": {
        "apigen/apigen": "dev-issue-fix-tree"
    },
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "apigen/apigen",
                "version": "dev-issue-fix-tree",
                "dist": {
                    "url": "https://github.com/markstory/apigen/archive/issue-fix-tree.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/markstory/apigen",
                    "type": "git",
                    "reference": "issue-fix-tree"
                },
                "require": {
                    "php": ">=5.5",
                    "andrewsville/php-token-reflection": "^1.4",
                    "apigen/theme-bootstrap": "1.1.*",
                    "apigen/theme-default": "1.0.*",
                    "herrera-io/phar-update": "~2.0",
                    "kukulich/fshl": "~2.1",
                    "latte/latte": "^2.3.6",
                    "michelf/php-markdown": "~1.4",
                    "nette/bootstrap": "^2.3",
                    "nette/di": "^2.3",
                    "nette/neon": "^2.3",
                    "nette/reflection": "^2.3",
                    "symfony/console": "^2.7",
                    "symfony/event-dispatcher": "^2.7",
                    "symfony/options-resolver": "^2.7",
                    "symfony/yaml": "^2.7",
                    "tracy/tracy": "^2.3"
                },
                "autoload": {
                  "psr-4": {
                    "ApiGen\\": "src"
                  }
                },
                "bin": [
                  "bin/apigen"
                ]
            }
        }
    ]
}
